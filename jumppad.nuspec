  <metadata>
    <id>jumppad</id>
    <version>__version__</version>
    <packageSourceUrl>https://github.com/jumpppad-labs/choco</packageSourceUrl>
    <title>JumpPad</title>
    <authors>Erik Veld, Nic Jackson</authors>
    <projectUrl>https://jumppad.dev</projectUrl>
    <iconUrl>https://jumppad.dev/img/jumppad_no_text.png</iconUrl>
